{
  "name": "suru-plus-folders",
  "private": true,
  "description": "Suru++ Folder is a bash script that allows changing the color of folders in the families Adwaita, Suru and Yaru",
  "version": "4.0.0",
  "author": "Gustavo Reis",
  "keywords": [
    "adwaita",
    "adwaita-plus",
    "color",
    "colors",
    "colour",
    "colours",
    "customisation",
    "customization",
    "folder",
    "folders",
    "gatsby",
    "golang",
    "hugo",
    "linux",
    "suru",
    "suru-plus",
    "ubuntu",
    "yaru",
    "yaru-plus"
  ],
  "license": "MIT+",
  "browserslist": "> 1%",
  "scripts": {
    "start": "run-p start:*",
    "start:hugo": "hugo server -w --config 'config.yaml' -l 'layouts' -b gusbemacbe.pt -p 1989",
    "start:webpack": "webpack --mode=production --watch",
    "start:webpack-dev-server": "webpack-dev-server",
    "build": "run-s build:webpack build:webpack-dev-server build:hugo",
    "build:hugo": "hugo",
    "build:webpack": "webpack --mode=production --watch",
    "build:webpack-dev-server": "webpack-dev-server --port 1989 --open firefox --watch",
    "convertir": "cd ferramentas && node convparacss.js",
    "ordenar": "cd ferramentas && node ordenar-as-cores.js",
    "formatar": "prettier --trailing-comma es5 --no-semi --single-quote --write \"**/*.{html,md}\"",
    "teste-jest": "jest"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/.cache/",
      "/node_modules/",
      "/public/"
    ]
  },
  "dependencies": {
    "@mdx-js/react": "1.6.16",
    "@types/autoprefixer": "^9.7.2",
    "chalk": "2.4.2",
    "fs": "0.0.1-security",
    "gh-pages": "3.1.0",
    "hexsorter": "^1.5.0",
    "intl": "1.2.5",
    "palx": "^1.0.3",
    "prismjs": "1.21.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-intl": "2.7.0",
    "react-live": "2.2.2",
    "react-router-dom": "5.2.0",
    "readline": "^1.3.0",
    "sematable": "^2.7.0",
    "theme-ui": "0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@mdx-js/loader": "^1.6.17",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "codecov": "3.7.2",
    "core-js": "^3.6.5",
    "css": "3.0.0",
    "css-loader": "^4.3.0",
    "csscomb": "^4.3.0",
    "eslint": "7.8.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.1.0",
    "jest": "26.4.2",
    "mini-css-extract-plugin": "^0.11.2",
    "npm-run-all": "4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.1.2",
    "prettier-plugin-go-template": "0.0.10",
    "prettier-webpack-plugin": "^1.2.0",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "typescript": "4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
